jira.Board SKP_board@s {
  name = "SKP board"
  type = "kanban"
  admins = {
    users = [
      {
        id = "557058:e6dea0b2-de5c-4e06-9b5b-c60809b0df8f"
        displayName = "Alex Ortiz"
      },
    ]
  }
  location = {
    projectId = jira.Project.instance.Sample_Kanban_Project@s
  }
  filterId = jira.Filter.instance.Filter_for_SKP_board@s
  columnConfig = {
    columns = [
      {
        name = "Backlog"
        statuses = [
          jira.Status.instance.backlog,
        ]
      },
      {
        name = "DEV::Selected for Development"
        min = 1
        statuses = [
          jira.Status.instance.selected_for_development@s,
        ]
        max = 3
      },
      {
        name = "DEV::In Progress"
        max = 6
        statuses = [
          jira.Status.instance.in_progress@s,
        ]
        min = 1
      },
      {
        name = "Done"
        statuses = [
          jira.Status.instance.done,
        ]
      },
    ]
    constraintType = "issueCount"
  }
  subQuery = "fixVersion in unreleasedVersions() OR fixVersion is EMPTY"
}
